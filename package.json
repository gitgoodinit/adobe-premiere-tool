{
  "name": "audio-tools-premiere-plugin",
  "version": "1.0.0",
  "description": "Audio Tools Pro - Phase 1: Silence Detection & Trimming for Adobe Premiere Pro",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/dev-server.js",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "setup:ffmpeg": "node scripts/setup-ffmpeg.js",
    "setup:env": "node scripts/setup-env.js",
    "package": "node scripts/package-plugin.js"
  },
  "keywords": [
    "adobe",
    "premiere-pro",
    "audio",
    "silence-detection",
    "cep",
    "extendscript",
    "ffmpeg",
    "whisper",
    "audio-processing"
  ],
  "author": "Azeem",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "file-loader": "^6.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "html-webpack-plugin": "^5.5.3",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@jest/globals": "^29.7.0"
  },
  "optionalDependencies": {
    "fluent-ffmpeg": "^2.1.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "cep": {
    "manifestVersion": "7.0",
    "bundleId": "com.audiotools.premiereaudioplugin",
    "bundleVersion": "1.0.0",
    "bundleName": "Audio Tools Pro",
    "hostList": [
      {
        "name": "PPRO",
        "version": "[0.0,99.9]"
      }
    ],
    "requiredRuntime": {
      "name": "CSXS",
      "version": "12.0"
    }
  },
  "apis": {
    "openai": {
      "endpoint": "https://api.openai.com/v1/audio/transcriptions",
      "model": "whisper-1",
      "required": false
    },
    "google": {
      "endpoint": "https://speech.googleapis.com/v1/speech:recognize",
      "model": "latest_long",
      "required": false
    }
  },
  "ffmpeg": {
    "version": ">=4.0.0",
    "binaries": {
      "win32": "./bin/ffmpeg.exe",
      "darwin": "/usr/local/bin/ffmpeg",
      "linux": "/usr/bin/ffmpeg"
    },
    "required": false
  },
  "directories": {
    "src": "./src",
    "dist": "./dist",
    "test": "./test",
    "docs": "./docs",
    "bin": "./bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/audio-tools-premiere-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/audio-tools-premiere-plugin/issues"
  },
  "homepage": "https://github.com/your-username/audio-tools-premiere-plugin#readme",
  "config": {
    "development": {
      "mockFFmpeg": true,
      "mockAPIs": true,
      "enableDebugConsole": true,
      "logLevel": "debug"
    },
    "production": {
      "mockFFmpeg": false,
      "mockAPIs": false,
      "enableDebugConsole": false,
      "logLevel": "info"
    }
  }
}